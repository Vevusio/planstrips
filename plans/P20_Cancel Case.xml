<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plan-library SYSTEM "/Users/kathi/Documents/BRIGID/Asbru Interpreter/dtd/Asbru_7.3h_DELTA.dtd">
<plan-library>
	<library-info title="NewPlanLibrary">
		<administrative-data original-author="KK"/>
	</library-info>
	<domain-defs>
		<domain name="mainDomain">
			<qualitative-scale-def name="Boolean">
				<qualitative-entry entry="no" />
				<qualitative-entry entry="yes" />
			</qualitative-scale-def>
			<parameter-group>
				<parameter-def name="kill" required="no" type="Boolean">
					<raw-data-def mode="manual" use-as-context="no" />
				</parameter-def>
			</parameter-group>
		</domain>
	</domain-defs>
  <plans>
  	<plan-group>
  		<plan name="root_P20" title="Cancel Activity">
  			<conditions>
  				<abort-condition>
  					<parameter-proposition parameter-name="kill">
  						<value-description type="equal">
  							<qualitative-constant value="yes" />
  						</value-description>
  						<context>
  							<any />
  						</context>
  						<time-annotation>
  							<now />
  						</time-annotation>
  					</parameter-proposition>
  				</abort-condition>
  			</conditions>
  			<plan-body>
  				<subplans type="sequentially">
  					<wait-for>
  						<all/>
  					</wait-for>
  					<plan-activation>
  						<plan-schema name="Pre_Block" />
  					</plan-activation>
  					<plan-activation>
  						<plan-schema name="Block" />
  					</plan-activation>
  					<plan-activation>
  						<plan-schema name="Post_Block" />
  					</plan-activation>
  				</subplans>
  			</plan-body>
  		</plan>
		<plan name="Block" title="Block">
  			<plan-body>
  				<subplans type="unordered" wait-for-optional-subplans="yes">
  					<wait-for>
  						<none/>
  					</wait-for>
  					<plan-activation>
  						<plan-schema name="Plan_A" />
  					</plan-activation>
  					<plan-activation>
  						<plan-schema name="Plan_B" />
  					</plan-activation>
  					<plan-activation>
  						<plan-schema name="Plan_C" />
  					</plan-activation>
  				</subplans>
  			</plan-body>
  		</plan>
  		<plan name="Plan_A">
  			<conditions>
  				<complete-condition>
						<plan-state-constraint state="activated">
							<self />
							<time-annotation>
                            	<time-range>
                            		<starting-shift>
                            			<latest>
                            				<numerical-constant value="-1" unit="s" />
                            			</latest>
									</starting-shift>
								</time-range>
                            	<now />
                            </time-annotation>
						</plan-state-constraint>
					</complete-condition>
  			</conditions>
  			<plan-body>
  				<user-performed />
  			</plan-body>
  		</plan>
  		<plan name="Plan_B">
  			<conditions>
  				<setup-precondition>
						<plan-state-constraint state="selected">
							<self />
							<time-annotation>
                            	<time-range>
                            		<starting-shift>
                            			<latest>
                            				<numerical-constant value="-2" unit="s" />
                            			</latest>
									</starting-shift>
								</time-range>
                            	<now />
                            </time-annotation>
						</plan-state-constraint>
					</setup-precondition>
  				<complete-condition>
						<plan-state-constraint state="activated">
							<self />
							<time-annotation>
                            	<time-range>
                            		<starting-shift>
                            			<latest>
                            				<numerical-constant value="-2" unit="s" />
                            			</latest>
									</starting-shift>
								</time-range>
                            	<now />
                            </time-annotation>
						</plan-state-constraint>
					</complete-condition>
  			</conditions>
  			<plan-body>
  				<user-performed />
  			</plan-body>
  		</plan>
  		<plan name="Plan_C">
  			<conditions>
  				<complete-condition>
						<plan-state-constraint state="activated">
							<self />
							<time-annotation>
                            	<time-range>
                            		<starting-shift>
                            			<latest>
                            				<numerical-constant value="-2" unit="s" />
                            			</latest>
									</starting-shift>
								</time-range>
                            	<now />
                            </time-annotation>
						</plan-state-constraint>
					</complete-condition>
  			</conditions>
  			<plan-body>
  				<user-performed />
  			</plan-body>
  		</plan>
  		<plan name="Pre_Block">
  			<conditions>
  				<complete-condition>
						<plan-state-constraint state="activated">
							<self />
							<time-annotation>
                            	<time-range>
                            		<starting-shift>
                            			<latest>
                            				<numerical-constant value="-1" unit="s" />
                            			</latest>
									</starting-shift>
								</time-range>
                            	<now />
                            </time-annotation>
						</plan-state-constraint>
					</complete-condition>
  			</conditions>
  			<plan-body>
  				<user-performed />
  			</plan-body>
  		</plan>
  		<plan name="Post_Block">
  			<plan-body>
  				<user-performed />
  			</plan-body>
  		</plan>
  	</plan-group>
  </plans>
</plan-library>
